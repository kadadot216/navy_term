#include <stdlib.h>
#include "types.h"

void	init_board_cell(board_cell_t *cell)
{
	cell->coords[0] = 0;
	cell->coords[1] = 0;
	cell->x = 0;
	cell->y = 0;
	cell->dp_t = NONE;

}

